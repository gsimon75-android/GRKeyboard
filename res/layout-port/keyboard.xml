<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
	xmlns:a="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	a:id="@+id/keyboard" a:orientation="vertical"
	a:layout_width="fill_parent" a:layout_height="wrap_content">

	<LinearLayout a:id="@+id/row_0" a:orientation="horizontal"
		a:layout_width="fill_parent" a:layout_height="wrap_content">
		<org.dyndns.fules.grkey.GRKey a:id="@+id/key_Backtick"  style="@style/GRKey"/>
		<org.dyndns.fules.grkey.GRKey a:id="@+id/key_1"         style="@style/GRKey"/>
		<org.dyndns.fules.grkey.GRKey a:id="@+id/key_2"         style="@style/GRKey"/>
		<org.dyndns.fules.grkey.GRKey a:id="@+id/key_3"         style="@style/GRKey"/>
		<org.dyndns.fules.grkey.GRKey a:id="@+id/key_4"         style="@style/GRKey"/>
		<org.dyndns.fules.grkey.GRKey a:id="@+id/key_5"         style="@style/GRKey"/>
		<org.dyndns.fules.grkey.GRKey a:id="@+id/key_6"         style="@style/GRKey"/>
		<org.dyndns.fules.grkey.GRKey a:id="@+id/key_7"         style="@style/GRKey"/>
		<org.dyndns.fules.grkey.GRKey a:id="@+id/key_8"         style="@style/GRKey"/>
		<org.dyndns.fules.grkey.GRKey a:id="@+id/key_9"         style="@style/GRKey"/>
		<org.dyndns.fules.grkey.GRKey a:id="@+id/key_0"         style="@style/GRKey"/>
		<org.dyndns.fules.grkey.GRKey a:id="@+id/key_Backspace" style="@style/GRKey"/>
	</LinearLayout>

	<LinearLayout a:id="@+id/row_1" a:orientation="horizontal"
		a:layout_width="fill_parent" a:layout_height="wrap_content">
		<org.dyndns.fules.grkey.GRKey a:id="@+id/key_Q"         style="@style/GRKey"/>
		<org.dyndns.fules.grkey.GRKey a:id="@+id/key_W"         style="@style/GRKey"/>
		<org.dyndns.fules.grkey.GRKey a:id="@+id/key_E"         style="@style/GRKey"/>
		<org.dyndns.fules.grkey.GRKey a:id="@+id/key_R"         style="@style/GRKey"/>
		<org.dyndns.fules.grkey.GRKey a:id="@+id/key_T"         style="@style/GRKey"/>
		<org.dyndns.fules.grkey.GRKey a:id="@+id/key_Y"         style="@style/GRKey"/>
		<org.dyndns.fules.grkey.GRKey a:id="@+id/key_U"         style="@style/GRKey"/>
		<org.dyndns.fules.grkey.GRKey a:id="@+id/key_I"         style="@style/GRKey"/>
		<org.dyndns.fules.grkey.GRKey a:id="@+id/key_O"         style="@style/GRKey"/>
		<org.dyndns.fules.grkey.GRKey a:id="@+id/key_P"         style="@style/GRKey"/>
		<org.dyndns.fules.grkey.GRKey a:id="@+id/key_OpenSquare" style="@style/GRKey"/>
		<org.dyndns.fules.grkey.GRKey a:id="@+id/key_CloseSquare" style="@style/GRKey"/>
	</LinearLayout>

	<LinearLayout a:id="@+id/row_2" a:orientation="horizontal"
		a:layout_width="fill_parent" a:layout_height="wrap_content">
		<Space a:layout_width="0px" a:layout_height="0px" a:layout_weight="0.3"/>
		<org.dyndns.fules.grkey.GRKey a:id="@+id/key_A"         style="@style/GRKey"/>
		<org.dyndns.fules.grkey.GRKey a:id="@+id/key_S"         style="@style/GRKey"/>
		<org.dyndns.fules.grkey.GRKey a:id="@+id/key_D"         style="@style/GRKey"/>
		<org.dyndns.fules.grkey.GRKey a:id="@+id/key_F"         style="@style/GRKey"/>
		<org.dyndns.fules.grkey.GRKey a:id="@+id/key_G"         style="@style/GRKey"/>
		<org.dyndns.fules.grkey.GRKey a:id="@+id/key_H"         style="@style/GRKey"/>
		<org.dyndns.fules.grkey.GRKey a:id="@+id/key_J"         style="@style/GRKey"/>
		<org.dyndns.fules.grkey.GRKey a:id="@+id/key_K"         style="@style/GRKey"/>
		<org.dyndns.fules.grkey.GRKey a:id="@+id/key_L"         style="@style/GRKey"/>
		<org.dyndns.fules.grkey.GRKey a:id="@+id/key_Semicolon" style="@style/GRKey"/>
		<org.dyndns.fules.grkey.GRKey a:id="@+id/key_Apostrophe" style="@style/GRKey"/>
		<org.dyndns.fules.grkey.GRKey a:id="@+id/key_Enter"     style="@style/GRKey" a:layout_weight="0.8"/>
	</LinearLayout>

	<LinearLayout a:id="@+id/row_3" a:orientation="horizontal"
		a:layout_width="fill_parent" a:layout_height="wrap_content">
		<Space a:layout_width="0px" a:layout_height="0px" a:layout_weight="0.6"/>
		<org.dyndns.fules.grkey.GRKey a:id="@+id/key_Z"         style="@style/GRKey"/>
		<org.dyndns.fules.grkey.GRKey a:id="@+id/key_X"         style="@style/GRKey"/>
		<org.dyndns.fules.grkey.GRKey a:id="@+id/key_C"         style="@style/GRKey"/>
		<org.dyndns.fules.grkey.GRKey a:id="@+id/key_V"         style="@style/GRKey"/>
		<org.dyndns.fules.grkey.GRKey a:id="@+id/key_B"         style="@style/GRKey"/>
		<org.dyndns.fules.grkey.GRKey a:id="@+id/key_N"         style="@style/GRKey"/>
		<org.dyndns.fules.grkey.GRKey a:id="@+id/key_M"         style="@style/GRKey"/>
		<org.dyndns.fules.grkey.GRKey a:id="@+id/key_Comma"         style="@style/GRKey"/>
		<org.dyndns.fules.grkey.GRKey a:id="@+id/key_Dot"       style="@style/GRKey"/>
		<org.dyndns.fules.grkey.GRKey a:id="@+id/key_Slash"         style="@style/GRKey"/>
		<org.dyndns.fules.grkey.GRKey a:id="@+id/key_Up"        style="@style/GRKey"/>
		<org.dyndns.fules.grkey.GRKey a:id="@+id/key_Enter"     style="@style/GRKey"/>
	</LinearLayout>

		<!--Space a:layout_width="0px" a:layout_height="0px" a:layout_weight="0.5"/-->
	<LinearLayout a:id="@+id/row_4" a:orientation="horizontal"
		a:paddingBottom="2mm"
		a:layout_width="fill_parent" a:layout_height="wrap_content">
		<org.dyndns.fules.grkey.GRKey a:id="@+id/key_Shift"     style="@style/GRKey" a:layout_weight="2.4"/>
		<org.dyndns.fules.grkey.GRKey a:id="@+id/key_Space"     style="@style/GRKey" a:layout_weight="6"/>
		<org.dyndns.fules.grkey.GRKey a:id="@+id/key_Left"      style="@style/GRKey"/>
		<org.dyndns.fules.grkey.GRKey a:id="@+id/key_Down"      style="@style/GRKey"/>
		<org.dyndns.fules.grkey.GRKey a:id="@+id/key_Right"     style="@style/GRKey"/>
	</LinearLayout>

</LinearLayout>
