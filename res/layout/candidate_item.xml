<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
	xmlns:a="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	a:orientation="vertical" a:layout_width="wrap_content" a:layout_height="wrap_content">

	<org.dyndns.fules.grkey.GestureView a:id="@+id/candidateGesture" a:layout_width="40dp" a:layout_height="40dp" a:padding="10dp"
		app:circleRadius="3dp" app:arrowLength="6dp" app:lineWidth="2dp" app:outlineWidth="1dp" a:layout_gravity="center_horizontal"/>
	<TextView a:id="@+id/candidateText" a:layout_width="wrap_content" a:layout_height="wrap_content" a:layout_gravity="center_horizontal"
		a:textColor="@android:color/primary_text_dark" a:textSize="20sp"/>
</LinearLayout>
